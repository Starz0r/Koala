<Page
    x:Class="Koala.Views.MainPage"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:viewmodels="using:Koala.ViewModels"
    mc:Ignorable="d">
    <Grid>
        <!-- <SwapChainPanel x:Name="videoBox" CanDrag="False"/> -->
        <viewmodels:MediaElement x:Name="VideoPlayer" AreTransportControlsEnabled="True">
            <viewmodels:MediaElement.TransportControls>
                <viewmodels:MediaTransportControls x:Name="VideoControls" IsZoomEnabled="False" IsZoomButtonVisible="False"></viewmodels:MediaTransportControls>
            </viewmodels:MediaElement.TransportControls>
        </viewmodels:MediaElement>
    </Grid>
</Page>
